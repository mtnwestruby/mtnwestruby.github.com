<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>MountainWest RubyConf 2015</title>
 <link href="http://mtnwestrubyconf.org/2015/atom.xml" rel="self"/>
 <link href="http://mtnwestrubyconf.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mtnwestrubyconf.org/</id>

 {% for post in site.categories.2015 %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://mtnwestrubyconf.org/2015/#{{ post.id | bookmark }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mtnwestrubyconf.org/2015/#{{ post.id | bookmark }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
